<h1>Create a new Bookmarks</h1>

<div class="row">
  <div class="form-group">
    <%= form_for [@topic, @bookmark] do |f| %>
  </div>
  <div class="form-group">
    <%= f.label :url %>
    <%= f.text_field :url %>
  </div>
  <div class="form-group">
    <%= f.submit "Save" %>
  </div>
  <% end %>
</div>
